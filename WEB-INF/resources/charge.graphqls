type Charge {
  id: ID!
  client: Client!
  organization: Organization!
  isActive: Boolean!
  created: String! @dateFormat
  createdBy: User!
  description: String
  name: String
  isLocked: Boolean
  account: Account
}

input ChargeInput {
  id: ID
  isActive: Boolean!
  description: String
  name: String
  isLocked: Boolean
  account: AccountInput
}