type User {
  name: String!
  id: ID!
}

type Client {
  name: String!
  id: ID!
  organizations: [Organization!]!
}

type Organization {
  name: String!
  id: ID!
  roles: [Role!]!
  warehouses: [Warehouse!]!
}

type Role {
  name: String!
  id: ID!
}

type Warehouse {
  name: String!
  id: ID!
}

type Query {
  getUsers: [User]
}

type Mutation {
  _empty: String
}

schema {
  query: Query
  mutation: Mutation
}
