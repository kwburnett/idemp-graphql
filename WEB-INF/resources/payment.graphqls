type Payment {
  id: ID!
  client: Client!
  organization: Organization!
  isActive: Boolean!
  created: String @dateFormat
  createdBy: User!
  businessPartner: BusinessPartner
  charge: Charge
  order: Order
  payAmount: Float
  paymentType: ReferenceList
  nhifType: ReferenceList
  nhifRelationship: ReferenceList
  claimNumber: String
  memberId: String
  number: String
  memberName: String
  docStatus: DocStatus
  transactionDate: String @dateFormat
  tenderAmount: Float
}